MODULE main
VAR
	dir: {up, left, down, right};
	bl_2_2_X: 0..7;
	bl_2_2_Y: 0..7;
	bl_4_3_X: 0..7;
	bl_4_3_Y: 0..7;
	bl_5_4_X: 0..7;
	bl_5_4_Y: 0..7;
	mX: 0..7;
	mY: 0..7;
DEFINE
	dx := case
		dir = left: -1;
		dir = right: 1;
		TRUE: 0;
		esac;
	dy := case
		dir = up: -1;
		dir = down: 1;
		TRUE: 0;
		esac;
INIT
bl_2_2_X=2 & bl_2_2_Y=2 & bl_4_3_X=4 & bl_4_3_Y=3 & mX=2 & mY=4 & bl_5_4_X=5 & bl_5_4_Y=4;
INVAR
	--the blocks and the man should stay within bounds
	((bl_2_2_X>=0 & bl_2_2_X<=7) & (bl_2_2_Y>=0 & bl_2_2_Y<=7))
	& ((bl_4_3_X>=0 & bl_4_3_X<=7) & (bl_4_3_Y>=0 & bl_4_3_Y<=7))
	& ((bl_5_4_X>=0 & bl_5_4_X<=7) & (bl_5_4_Y>=0 & bl_5_4_Y<=7))
	& (mX>=0 & mX<=7) & (mY>=0 & mY<=7);
INVAR
	--the blocks and the man can not overlap with the walls
	!(bl_2_2_X=0 & bl_2_2_Y=0) & !(bl_4_3_X=0 & bl_4_3_Y=0) & !(bl_5_4_X=0 & bl_5_4_Y=0) & !(mX=0 & mY=0)
	& !(bl_2_2_X=1 & bl_2_2_Y=0) & !(bl_4_3_X=1 & bl_4_3_Y=0) & !(bl_5_4_X=1 & bl_5_4_Y=0) & !(mX=1 & mY=0)
	& !(bl_2_2_X=2 & bl_2_2_Y=0) & !(bl_4_3_X=2 & bl_4_3_Y=0) & !(bl_5_4_X=2 & bl_5_4_Y=0) & !(mX=2 & mY=0)
	& !(bl_2_2_X=3 & bl_2_2_Y=0) & !(bl_4_3_X=3 & bl_4_3_Y=0) & !(bl_5_4_X=3 & bl_5_4_Y=0) & !(mX=3 & mY=0)
	& !(bl_2_2_X=4 & bl_2_2_Y=0) & !(bl_4_3_X=4 & bl_4_3_Y=0) & !(bl_5_4_X=4 & bl_5_4_Y=0) & !(mX=4 & mY=0)
	& !(bl_2_2_X=5 & bl_2_2_Y=0) & !(bl_4_3_X=5 & bl_4_3_Y=0) & !(bl_5_4_X=5 & bl_5_4_Y=0) & !(mX=5 & mY=0)
	& !(bl_2_2_X=6 & bl_2_2_Y=0) & !(bl_4_3_X=6 & bl_4_3_Y=0) & !(bl_5_4_X=6 & bl_5_4_Y=0) & !(mX=6 & mY=0)
	& !(bl_2_2_X=7 & bl_2_2_Y=0) & !(bl_4_3_X=7 & bl_4_3_Y=0) & !(bl_5_4_X=7 & bl_5_4_Y=0) & !(mX=7 & mY=0)
	& !(bl_2_2_X=0 & bl_2_2_Y=1) & !(bl_4_3_X=0 & bl_4_3_Y=1) & !(bl_5_4_X=0 & bl_5_4_Y=1) & !(mX=0 & mY=1)
	& !(bl_2_2_X=4 & bl_2_2_Y=1) & !(bl_4_3_X=4 & bl_4_3_Y=1) & !(bl_5_4_X=4 & bl_5_4_Y=1) & !(mX=4 & mY=1)
	& !(bl_2_2_X=5 & bl_2_2_Y=1) & !(bl_4_3_X=5 & bl_4_3_Y=1) & !(bl_5_4_X=5 & bl_5_4_Y=1) & !(mX=5 & mY=1)
	& !(bl_2_2_X=6 & bl_2_2_Y=1) & !(bl_4_3_X=6 & bl_4_3_Y=1) & !(bl_5_4_X=6 & bl_5_4_Y=1) & !(mX=6 & mY=1)
	& !(bl_2_2_X=7 & bl_2_2_Y=1) & !(bl_4_3_X=7 & bl_4_3_Y=1) & !(bl_5_4_X=7 & bl_5_4_Y=1) & !(mX=7 & mY=1)
	& !(bl_2_2_X=0 & bl_2_2_Y=2) & !(bl_4_3_X=0 & bl_4_3_Y=2) & !(bl_5_4_X=0 & bl_5_4_Y=2) & !(mX=0 & mY=2)
	& !(bl_2_2_X=7 & bl_2_2_Y=2) & !(bl_4_3_X=7 & bl_4_3_Y=2) & !(bl_5_4_X=7 & bl_5_4_Y=2) & !(mX=7 & mY=2)
	& !(bl_2_2_X=0 & bl_2_2_Y=3) & !(bl_4_3_X=0 & bl_4_3_Y=3) & !(bl_5_4_X=0 & bl_5_4_Y=3) & !(mX=0 & mY=3)
	& !(bl_2_2_X=3 & bl_2_2_Y=3) & !(bl_4_3_X=3 & bl_4_3_Y=3) & !(bl_5_4_X=3 & bl_5_4_Y=3) & !(mX=3 & mY=3)
	& !(bl_2_2_X=5 & bl_2_2_Y=3) & !(bl_4_3_X=5 & bl_4_3_Y=3) & !(bl_5_4_X=5 & bl_5_4_Y=3) & !(mX=5 & mY=3)
	& !(bl_2_2_X=7 & bl_2_2_Y=3) & !(bl_4_3_X=7 & bl_4_3_Y=3) & !(bl_5_4_X=7 & bl_5_4_Y=3) & !(mX=7 & mY=3)
	& !(bl_2_2_X=0 & bl_2_2_Y=4) & !(bl_4_3_X=0 & bl_4_3_Y=4) & !(bl_5_4_X=0 & bl_5_4_Y=4) & !(mX=0 & mY=4)
	& !(bl_2_2_X=7 & bl_2_2_Y=4) & !(bl_4_3_X=7 & bl_4_3_Y=4) & !(bl_5_4_X=7 & bl_5_4_Y=4) & !(mX=7 & mY=4)
	& !(bl_2_2_X=0 & bl_2_2_Y=5) & !(bl_4_3_X=0 & bl_4_3_Y=5) & !(bl_5_4_X=0 & bl_5_4_Y=5) & !(mX=0 & mY=5)
	& !(bl_2_2_X=1 & bl_2_2_Y=5) & !(bl_4_3_X=1 & bl_4_3_Y=5) & !(bl_5_4_X=1 & bl_5_4_Y=5) & !(mX=1 & mY=5)
	& !(bl_2_2_X=2 & bl_2_2_Y=5) & !(bl_4_3_X=2 & bl_4_3_Y=5) & !(bl_5_4_X=2 & bl_5_4_Y=5) & !(mX=2 & mY=5)
	& !(bl_2_2_X=3 & bl_2_2_Y=5) & !(bl_4_3_X=3 & bl_4_3_Y=5) & !(bl_5_4_X=3 & bl_5_4_Y=5) & !(mX=3 & mY=5)
	& !(bl_2_2_X=4 & bl_2_2_Y=5) & !(bl_4_3_X=4 & bl_4_3_Y=5) & !(bl_5_4_X=4 & bl_5_4_Y=5) & !(mX=4 & mY=5)
	& !(bl_2_2_X=5 & bl_2_2_Y=5) & !(bl_4_3_X=5 & bl_4_3_Y=5) & !(bl_5_4_X=5 & bl_5_4_Y=5) & !(mX=5 & mY=5)
	& !(bl_2_2_X=6 & bl_2_2_Y=5) & !(bl_4_3_X=6 & bl_4_3_Y=5) & !(bl_5_4_X=6 & bl_5_4_Y=5) & !(mX=6 & mY=5)
	& !(bl_2_2_X=7 & bl_2_2_Y=5) & !(bl_4_3_X=7 & bl_4_3_Y=5) & !(bl_5_4_X=7 & bl_5_4_Y=5) & !(mX=7 & mY=5)
	& !(bl_2_2_X=0 & bl_2_2_Y=6) & !(bl_4_3_X=0 & bl_4_3_Y=6) & !(bl_5_4_X=0 & bl_5_4_Y=6) & !(mX=0 & mY=6)
	& !(bl_2_2_X=1 & bl_2_2_Y=6) & !(bl_4_3_X=1 & bl_4_3_Y=6) & !(bl_5_4_X=1 & bl_5_4_Y=6) & !(mX=1 & mY=6)
	& !(bl_2_2_X=2 & bl_2_2_Y=6) & !(bl_4_3_X=2 & bl_4_3_Y=6) & !(bl_5_4_X=2 & bl_5_4_Y=6) & !(mX=2 & mY=6)
	& !(bl_2_2_X=3 & bl_2_2_Y=6) & !(bl_4_3_X=3 & bl_4_3_Y=6) & !(bl_5_4_X=3 & bl_5_4_Y=6) & !(mX=3 & mY=6)
	& !(bl_2_2_X=4 & bl_2_2_Y=6) & !(bl_4_3_X=4 & bl_4_3_Y=6) & !(bl_5_4_X=4 & bl_5_4_Y=6) & !(mX=4 & mY=6)
	& !(bl_2_2_X=5 & bl_2_2_Y=6) & !(bl_4_3_X=5 & bl_4_3_Y=6) & !(bl_5_4_X=5 & bl_5_4_Y=6) & !(mX=5 & mY=6)
	& !(bl_2_2_X=6 & bl_2_2_Y=6) & !(bl_4_3_X=6 & bl_4_3_Y=6) & !(bl_5_4_X=6 & bl_5_4_Y=6) & !(mX=6 & mY=6)
	& !(bl_2_2_X=7 & bl_2_2_Y=6) & !(bl_4_3_X=7 & bl_4_3_Y=6) & !(bl_5_4_X=7 & bl_5_4_Y=6) & !(mX=7 & mY=6)
	& !(bl_2_2_X=0 & bl_2_2_Y=7) & !(bl_4_3_X=0 & bl_4_3_Y=7) & !(bl_5_4_X=0 & bl_5_4_Y=7) & !(mX=0 & mY=7)
	& !(bl_2_2_X=1 & bl_2_2_Y=7) & !(bl_4_3_X=1 & bl_4_3_Y=7) & !(bl_5_4_X=1 & bl_5_4_Y=7) & !(mX=1 & mY=7)
	& !(bl_2_2_X=2 & bl_2_2_Y=7) & !(bl_4_3_X=2 & bl_4_3_Y=7) & !(bl_5_4_X=2 & bl_5_4_Y=7) & !(mX=2 & mY=7)
	& !(bl_2_2_X=3 & bl_2_2_Y=7) & !(bl_4_3_X=3 & bl_4_3_Y=7) & !(bl_5_4_X=3 & bl_5_4_Y=7) & !(mX=3 & mY=7)
	& !(bl_2_2_X=4 & bl_2_2_Y=7) & !(bl_4_3_X=4 & bl_4_3_Y=7) & !(bl_5_4_X=4 & bl_5_4_Y=7) & !(mX=4 & mY=7)
	& !(bl_2_2_X=5 & bl_2_2_Y=7) & !(bl_4_3_X=5 & bl_4_3_Y=7) & !(bl_5_4_X=5 & bl_5_4_Y=7) & !(mX=5 & mY=7)
	& !(bl_2_2_X=6 & bl_2_2_Y=7) & !(bl_4_3_X=6 & bl_4_3_Y=7) & !(bl_5_4_X=6 & bl_5_4_Y=7) & !(mX=6 & mY=7)
	& !(bl_2_2_X=7 & bl_2_2_Y=7) & !(bl_4_3_X=7 & bl_4_3_Y=7) & !(bl_5_4_X=7 & bl_5_4_Y=7) & !(mX=7 & mY=7);
INVAR
	--Blocks can not overlap
	!(bl_2_2_X=bl_4_3_X & bl_2_2_Y=bl_4_3_Y)
	& !(bl_2_2_X=bl_5_4_X & bl_2_2_Y=bl_5_4_Y)
	& !(bl_4_3_X=bl_5_4_X & bl_4_3_Y=bl_5_4_Y)
	& TRUE;
TRANS
	  next(mX) = mX  + dx
	& next(mY) = mY  + dy
	& next(bl_2_2_X) = case 
		next(mX)=bl_2_2_X & next(mY)=bl_2_2_Y: bl_2_2_X+dx;
		TRUE: bl_2_2_X;
		esac
	& next(bl_2_2_Y) = case 
		next(mX)=bl_2_2_X & next(mY)=bl_2_2_Y: bl_2_2_Y+dy;
		TRUE: bl_2_2_Y;
		esac
	& next(bl_4_3_X) = case 
		next(mX)=bl_4_3_X & next(mY)=bl_4_3_Y: bl_4_3_X+dx;
		TRUE: bl_4_3_X;
		esac
	& next(bl_4_3_Y) = case 
		next(mX)=bl_4_3_X & next(mY)=bl_4_3_Y: bl_4_3_Y+dy;
		TRUE: bl_4_3_Y;
		esac
	& next(bl_5_4_X) = case 
		next(mX)=bl_5_4_X & next(mY)=bl_5_4_Y: bl_5_4_X+dx;
		TRUE: bl_5_4_X;
		esac
	& next(bl_5_4_Y) = case 
		next(mX)=bl_5_4_X & next(mY)=bl_5_4_Y: bl_5_4_Y+dy;
		TRUE: bl_5_4_Y;
		esac;
INVARSPEC
	!(bl_2_2_X=2 & bl_2_2_Y=1) & !(bl_4_3_X=2 & bl_4_3_Y=1) & !(bl_5_4_X=2 & bl_5_4_Y=1)
	| !(bl_2_2_X=3 & bl_2_2_Y=1) & !(bl_4_3_X=3 & bl_4_3_Y=1) & !(bl_5_4_X=3 & bl_5_4_Y=1)
	| !(bl_2_2_X=4 & bl_2_2_Y=4) & !(bl_4_3_X=4 & bl_4_3_Y=4) & !(bl_5_4_X=4 & bl_5_4_Y=4);
